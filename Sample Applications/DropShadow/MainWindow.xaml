<Window x:Class="DropShadow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ink="clr-namespace:System.Windows.Ink;assembly=PresentationCore"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Background="Beige">
        <Canvas RenderTransform="1,0,0,1,3,3">
            <Canvas.Background>
                <VisualBrush
                    Visual="{Binding ElementName=inkCanvas1}"
                    Opacity="0.50" />
            </Canvas.Background>
        </Canvas>

        <InkCanvas Name="inkCanvas1" Background="Transparent">
            <InkCanvas.DefaultDrawingAttributes>
                <ink:DrawingAttributes Color="Black" Width="5" Height="5" />
            </InkCanvas.DefaultDrawingAttributes>

            <InkCanvas.Strokes>
                
            </InkCanvas.Strokes>
        </InkCanvas>

    </Grid>
</Window>